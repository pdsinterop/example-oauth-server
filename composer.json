{
  "autoload": {
    "psr-4": {
      "Pdsinterop\\Authentication\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "./bin",
    "platform": {
      "php": "7.2",
      "ext-mbstring": "1"
    }
  },
  "description": "Example OAuth2 implementation using the PHP League OAuth2 Server and Client packages",
  "license": "MIT",
  "name": "pdsinterop/oauth2-example",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "laminas/laminas-diactoros": "^2.3",
    "laminas/laminas-httphandlerrunner": "^1.2",
    "league/oauth2-client": "^2.5",
    "league/oauth2-server": "^8.0",
    "league/route": "^4.5"
  },
  "type": "library"
}
